<?xml version="1.0" encoding="utf-8"?>

<!-- Generated with http://android-holo-colors.com -->
<resources xmlns:android="http://schemas.android.com/apk/res/android">

  <style name="AutoCompleteTextViewUNDERTheme" parent="android:Widget.Holo.Light.AutoCompleteTextView">
      <item name="android:dropDownSelector">@drawable/undertheme_list_selector_holo_light</item>
	    <item name="android:background">@drawable/undertheme_edit_text_holo_light</item>
  </style>

  <style name="ButtonUNDERTheme" parent="android:Widget.Holo.Light.Button">
	  <item name="android:background">@drawable/undertheme_btn_default_holo_light</item>
  </style>

  <style name="ImageButtonUNDERTheme" parent="android:Widget.Holo.Light.ImageButton">
	  <item name="android:background">@drawable/undertheme_btn_default_holo_light</item>
  </style>

  <style name="SpinnerUNDERTheme" parent="android:Widget.Holo.Light.Spinner">
      <item name="android:background">@drawable/undertheme_spinner_background_holo_light</item>
      <item name="android:dropDownSelector">@drawable/undertheme_list_selector_holo_light</item>
  </style>

  <style name="TabUNDERTheme">
      <item name="android:gravity">center_horizontal</item>
      <item name="android:paddingLeft">16dip</item>
      <item name="android:paddingRight">16dip</item>
      <item name="android:background">@drawable/undertheme_tab_indicator_holo</item>
      <item name="android:layout_width">0dip</item>
      <item name="android:layout_weight">1</item>
      <item name="android:minWidth">80dip</item>
  </style>

  <style name="TabTextUNDERTheme">
	  <item name="android:textColor">#000000</item>
      <item name="android:textSize">12sp</item>
      <item name="android:textStyle">bold</item>
      <!-- v14 <item name="android:textAllCaps">true</item> -->
      <item name="android:ellipsize">marquee</item>
      <item name="android:maxLines">2</item>
      <item name="android:maxWidth">180dip</item>
  </style>

  <style name="ProgressBarUNDERTheme" parent="android:Widget.Holo.Light.ProgressBar.Horizontal">
      <item name="android:progressDrawable">@drawable/undertheme_progress_horizontal_holo_light</item>
      <item name="android:indeterminateDrawable">@drawable/undertheme_progress_indeterminate_horizontal_holo_light</item>
  </style>

  <style name="SeekBarUNDERTheme" parent="android:Widget.Holo.Light.SeekBar">
      <item name="android:progressDrawable">@drawable/undertheme_scrubber_progress_horizontal_holo_light</item>
      <item name="android:indeterminateDrawable">@drawable/undertheme_scrubber_progress_horizontal_holo_light</item>
      <item name="android:thumb">@drawable/undertheme_scrubber_control_selector_holo_light</item>
  </style>

  <style name="RatingBarUNDERTheme" parent="android:Widget.Holo.Light.RatingBar">
      <item name="android:progressDrawable">@drawable/undertheme_ratingbar_full_holo_light</item>
      <item name="android:indeterminateDrawable">@drawable/undertheme_ratingbar_full_holo_light</item>
  </style>

  <style name="RatingBarBigUNDERTheme" parent="android:Widget.Holo.Light.RatingBar.Indicator">
      <item name="android:progressDrawable">@drawable/undertheme_ratingbar_holo_light</item>
      <item name="android:indeterminateDrawable">@drawable/undertheme_ratingbar_holo_light</item>
  </style>

  <style name="RatingBarSmallUNDERTheme" parent="android:Widget.Holo.Light.RatingBar.Small">
      <item name="android:progressDrawable">@drawable/undertheme_ratingbar_small_holo_light</item>
      <item name="android:indeterminateDrawable">@drawable/undertheme_ratingbar_small_holo_light</item>
  </style>

  <style name="ToggleUNDERTheme" parent="android:Widget.Holo.Light.Button.Toggle">
      <item name="android:background">@drawable/undertheme_btn_toggle_holo_light</item>
  </style>

    <style name="UNDERTheme" parent="@style/_UNDERTheme">
        <item name="android:actionBarItemBackground">@drawable/selectable_background_undertheme</item>
        <item name="android:popupMenuStyle">@style/PopupMenu.UNDERTheme</item>
        <item name="android:dropDownListViewStyle">@style/DropDownListView.UNDERTheme</item>
        <item name="android:actionBarTabStyle">@style/ActionBarTabStyle.UNDERTheme</item>
        <item name="android:actionDropDownStyle">@style/DropDownNav.UNDERTheme</item>
        <item name="android:actionBarStyle">@style/ActionBar.Solid.UNDERTheme</item>
        <item name="android:actionModeBackground">@drawable/cab_background_top_undertheme</item>
        <item name="android:actionModeSplitBackground">@drawable/cab_background_bottom_undertheme</item>
        <item name="android:actionModeCloseButtonStyle">@style/ActionButton.CloseMode.UNDERTheme</item>

        <!-- Light.DarkActionBar specific -->
        <item name="android:actionBarWidgetTheme">@style/Theme.UNDERTheme.Widget</item>

    </style>

    <style name="ActionBar.Solid.UNDERTheme" parent="@android:style/Widget.Holo.Light.ActionBar.Solid.Inverse">
        <item name="android:background">@drawable/ab_solid_undertheme</item>
        <item name="android:backgroundStacked">@drawable/ab_stacked_solid_undertheme</item>
        <item name="android:backgroundSplit">@drawable/ab_bottom_solid_undertheme</item>
        <item name="android:progressBarStyle">@style/ProgressBar.UNDERTheme</item>
    </style>

    <style name="ActionBar.Transparent.UNDERTheme" parent="@android:style/Widget.Holo.ActionBar">
        <item name="android:background">@drawable/ab_transparent_undertheme</item>
        <item name="android:progressBarStyle">@style/ProgressBar.UNDERTheme</item>
    </style>

    <style name="PopupMenu.UNDERTheme" parent="@android:style/Widget.Holo.ListPopupWindow">
        <item name="android:popupBackground">@drawable/menu_dropdown_panel_undertheme</item>
    </style>

    <style name="DropDownListView.UNDERTheme" parent="@android:style/Widget.Holo.ListView.DropDown">
        <item name="android:listSelector">@drawable/selectable_background_undertheme</item>
    </style>

    <style name="ActionBarTabStyle.UNDERTheme" parent="@android:style/Widget.Holo.ActionBar.TabView">
        <item name="android:background">@drawable/tab_indicator_ab_undertheme</item>
    </style>

    <style name="DropDownNav.UNDERTheme" parent="@android:style/Widget.Holo.Spinner">
        <item name="android:background">@drawable/spinner_background_ab_undertheme</item>
        <item name="android:popupBackground">@drawable/menu_dropdown_panel_undertheme</item>
        <item name="android:dropDownSelector">@drawable/selectable_background_undertheme</item>
    </style>

    <style name="ProgressBar.UNDERTheme" parent="@android:style/Widget.Holo.ProgressBar.Horizontal">
        <item name="android:progressDrawable">@drawable/progress_horizontal_undertheme</item>
    </style>

    <style name="ActionButton.CloseMode.UNDERTheme" parent="@android:style/Widget.Holo.ActionButton.CloseMode">
        <item name="android:background">@drawable/btn_cab_done_undertheme</item>
    </style>

    <!-- this style is only referenced in a Light.DarkActionBar based theme -->
    <style name="Theme.UNDERTheme.Widget" parent="@android:style/Theme.Holo">
        <item name="android:popupMenuStyle">@style/PopupMenu.UNDERTheme</item>
        <item name="android:dropDownListViewStyle">@style/DropDownListView.UNDERTheme</item>
    </style>

</resources>